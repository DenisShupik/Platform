{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Swagger": {
      "IsCommonDocument": true,
      "CommonDocumentName": "YARP"
    },
    "Routes": {
      "coreServiceForums": {
        "ClusterId": "coreServiceCluster",
        "Match": {
          "Path": "/api/forums/{**catch-all}"
        }
      },
      "coreServiceCategories": {
        "ClusterId": "coreServiceCluster",
        "Match": {
          "Path": "/api/categories/{**catch-all}"
        }
      },
      "coreServiceThreads": {
        "ClusterId": "coreServiceCluster",
        "Match": {
          "Path": "/api/threads/{**catch-all}"
        }
      },
      "coreServicePosts": {
        "ClusterId": "coreServiceCluster",
        "Match": {
          "Path": "/api/posts/{**catch-all}"
        }
      },
      "userService": {
        "ClusterId": "userServiceCluster",
        "Match": {
          "Path": "/api/users/{**catch-all}"
        }
      },
      "fileService": {
        "ClusterId": "fileServiceCluster",
        "Match": {
          "Path": "/api/avatars/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "coreServiceCluster": {
        "Destinations": {
          "coreServiceDestination": {
            "Address": "https://localhost:8010",
            "Swaggers": [
              {
                "Paths": [
                  "/swagger/v1/swagger.json"
                ]
              }
            ]
          }
        }
      },
      "userServiceCluster": {
        "Destinations": {
          "userServiceDestination": {
            "Address": "https://localhost:8020",
            "Swaggers": [
              {
                "Paths": [
                  "/swagger/v1/swagger.json"
                ]
              }
            ]
          }
        }
      },
      "fileServiceCluster": {
        "Destinations": {
          "fileServiceDestination": {
            "Address": "https://localhost:8030",
            "Swaggers": [
              {
                "Paths": [
                  "/swagger/v1/swagger.json"
                ]
              }
            ]
          }
        }
      }
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Https": {
        "Protocols": "Http1AndHttp2",
        "Url": "https://+:8000"
      }
    }
  }
}
