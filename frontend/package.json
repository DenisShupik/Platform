{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"openapi-ts": "openapi-ts",
		"update-shadcn": "pnpm dlx shadcn-svelte@latest add --overwrite --yes avatar badge breadcrumb button card checkbox collapsible command dialog dropdown-menu form input input-group label pagination popover scroll-area select separator sheet switch textarea"
	},
	"devDependencies": {
		"@eslint/compat": "1.4.0",
		"@eslint/js": "9.37.0",
		"@hey-api/openapi-ts": "0.85.0",
		"@internationalized/date": "^3.10.0",
		"@lucide/svelte": "0.545.0",
		"@sveltejs/adapter-auto": "6.1.1",
		"@sveltejs/adapter-node": "5.3.3",
		"@sveltejs/kit": "2.46.1",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@tabler/icons-svelte": "3.35.0",
		"@tailwindcss/forms": "0.5.10",
		"@tailwindcss/typography": "0.5.19",
		"@tailwindcss/vite": "4.1.14",
		"bits-ui": "2.11.4",
		"clsx": "2.1.1",
		"eslint": "9.37.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.12.4",
		"formsnap": "2.0.1",
		"globals": "16.4.0",
		"keycloak-js": "26.2.0",
		"magic-string": "0.30.19",
		"mode-watcher": "1.1.0",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "3.4.0",
		"prettier-plugin-tailwindcss": "0.6.14",
		"svelte": "5.39.9",
		"svelte-check": "4.3.2",
		"sveltekit-superforms": "2.27.2",
		"tailwind-merge": "3.3.1",
		"tailwind-variants": "3.1.1",
		"tailwindcss": "4.1.14",
		"tw-animate-css": "^1.4.0",
		"typescript": "5.9.3",
		"typescript-eslint": "8.46.0",
		"valibot": "^1.1.0",
		"vite": "npm:rolldown-vite@7.1.16"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"@tailwindcss/oxide",
			"esbuild"
		]
	}
}
