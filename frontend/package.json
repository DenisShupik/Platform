		"svelte-check": "4.2.1",
		"sveltekit-superforms": "2.25.0",
		"tailwind-merge": "3.3.0",
		"tailwind-variants": "1.0.0",
		"tailwindcss": "4.1.7",
		"tailwindcss-animate": "1.0.7",
